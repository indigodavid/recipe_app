<div class = "mb-3 m-5">
  <p style="color: green"><%= notice %></p>
  <h1>Users</h1>
  <div id="users">
    <% @users.each do |user| %>
      <%= render user %>
      <p>
        <%= link_to "Show this user", user %>
      </p>
    <% end %>
  </div>
  <%= link_to "New user", new_user_path %>
</div>
