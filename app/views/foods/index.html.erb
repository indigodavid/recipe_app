<div class = "mb-3 m-5">
  <p style="color: green"><%= notice %></p>
  <h1>Foods</h1>
  <div id="foods">
    <% @foods.each do |food| %>
      <%= render food %>
      <p>
        <%= link_to "Show this food", food %>
      </p>
    <% end %>
  </div>
  <%= link_to "New food", new_food_path %>
</div>
